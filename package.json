{
  "name": "VendScreen-NetSuite-Scripts",
  "devDependencies": {
    "coffee-script": "~1.6",
    "browserify": "~3.18",
    "coffeeify": "~0.5",
    "should": "~2.1",
    "mocha": "~1.16",
    "browserify-shim": "~3.1.4",
    "brfs": "0.0.8",
    "handlebars": "~1.3.0",
    "memoizejs": "~0.1.0",
    "execSync": "~1.0.1",
    "coffeelint": "~1.1.0",
    "grunt": "~0.4.2",
    "grunt-coffeelint": "0.0.8",
    "node-lessify": "0.0.3",
    "pako": "~0.1.1",
    "uglify-js": "^2.4.13",
    "exec-sync": "^0.1.6"
  },
  "browser": {
    "jquery": "./vendor/jquery/jquery-1.10.2.js",
    "jquery-growl": "./vendor/jquery-growl/javascripts/jquery.growl.js",
    "jquery-growl-css": "./vendor/jquery-growl/stylesheets/jquery.growl.css",
    "es5-shim": "./vendor/es5-shim/es5-shim.js",
    "es5-sham": "./vendor/es5-shim/es5-sham.js",
    "jszip": "./vendor/jszip/lib/index.js"
  },
  "browserify-shim": {
    "jquery": {
      "depends": [
        "es5-shim",
        "es5-sham"
      ],
      "exports": "jQuery"
    },
    "jquery-growl": {
      "depends": [
        "jquery:jQuery",
        "jquery-growl-css"
      ],
      "exports": "jQuery.growl"
    },
    "memoizejs": {
      "exports": "memoize"
    }
  },
  "engine": {
    "node": ">=0.10.10"
  },
  "private": true,
  "repository": "https://bitbucket.org/johnogle/hydro-flask-netsuite-scripts.git",
  "readme": "NetSuite scripts for Hydro Flask"
}
